@model int

@{ 
    ViewBag.Title = "Record Not Found";
}

<div class="alert alert-danger mt-1 mb-1">
    <h4>404 Not Found Error: </h4>
    <hr />
    <h5>
        Employee with Id = @Model cannot be found!!!
    </h5>
</div>
<a asp-controller="Home" asp-action="Index" class="btn btn-outline-primary" style="width:auto">Employee List</a>